{
  "name": "ignite-express-ts",
  "version": "1.1.1",
  "description": "CLI to bootstrap Express + TypeScript apps quickly",
  "main": "dist/index.js",
  "bin": {
    "ignite-express-ts": "bin/index.js"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "start": "node dist/index.js"
  },
  "files": [
    "dist",
    "bin",
    "templates"
  ],
  "keywords": [
    "cli",
    "express",
    "typescript",
    "starter",
    "boilerplate"
  ],
  "author": "Mukul Padwal <mukulpadwal.me@gmail.com>",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "chalk": "^5.6.0",
    "figlet": "^1.8.2",
    "inquirer": "^12.9.4",
    "ora": "^8.2.0"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mukulpadwal/ignite-express-ts"
  },
  "homepage": "https://github.com/mukulpadwal/ignite-express-ts#readme",
  "bugs": {
    "url": "https://github.com/mukulpadwal/ignite-express-ts/issues"
  },
  "devDependencies": {
    "@types/figlet": "^1.7.0",
    "@types/node": "^24.3.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
